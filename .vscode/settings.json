{
    "workbench.colorTheme": "Quiet Light",
    "files.associations": {
        "*.cjson": "jsonc",
        "*.wxss": "css",
        "*.wxs": "javascript"
    },
    "emmet.includeLanguages": {
        "wxml": "html"
    },
    "minapp-vscode.disableAutoConfig": true,
    

    "less.compile": {

        "outExt": ".wxss"

    },
    "files.autoSave": "off",
    "emmet.triggerExpansionOnTab": true,
    "editor.wordWrap": "on"
}